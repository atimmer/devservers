{
  "name": "@24letters/devservers-shared",
  "version": "0.2.0",
  "type": "module",
  "description": "Shared types and schema for Devservers Manager.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atimmer/devservers"
  },
  "homepage": "https://github.com/atimmer/devservers#readme",
  "bugs": {
    "url": "https://github.com/atimmer/devservers/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "devservers",
    "schema",
    "types",
    "shared"
  ],
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src"
  },
  "dependencies": {
    "zod": "^4.3.5"
  }
}
